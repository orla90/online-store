{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es5", "es6", "dom", "DOM.Iterable", "es2021"],
    "module": "es2015",
    "moduleResolution": "node",
    "allowJs": true,
    "sourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
